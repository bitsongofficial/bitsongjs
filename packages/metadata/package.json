{
  "name": "@bitsongjs/metadata",
  "version": "0.1.3",
  "author": "Angelo Recca <angelorc@users.noreply.github.com>",
  "description": "A Javascript lib for BitSong Metadata",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "@bitsongjs:registry": "https://registry.npmjs.com"
  },
  "bugs": {
    "url": "https://github.com/bitsongofficial/bitsongjs/issues"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "test/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bitsongofficial/bitsongjs",
    "directory": "packages/metadata"
  },
  "directories": {
    "dist": "dist",
    "test": "test"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^28.1.8",
    "@types/uuid": "^9.0.7",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.104.0",
    "typescript": "^5.1.6",
    "unbuild": "^2.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm run dev && eslint .",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@cosmjs/encoding": "^0.32.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.3"
  }
}
